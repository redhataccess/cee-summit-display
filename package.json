    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.8",
    "browserify": "^13.0.1",
    "nodemon": "^1.9.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "springy": "^2.7.1"
  }
}
